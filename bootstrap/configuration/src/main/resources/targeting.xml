        <sv:value>com.onehippo.cms7.targeting.storage.elastic2.ElasticStoreFactory</sv:value>
      </sv:property>
      <sv:property sv:name="indexName" sv:type="String">
        <sv:value>visits</sv:value>
      </sv:property>
      <sv:property sv:name="locations" sv:type="String" sv:multiple="true">
        <sv:value>http://localhost:9200</sv:value>
      </sv:property>
    </sv:node>
    <sv:node sv:name="targeting:requestlog" esv:merge="combine">
      <sv:property sv:name="targeting:storefactoryclass" sv:type="String" esv:merge="override">
        <sv:value>com.onehippo.cms7.targeting.storage.elastic2.ElasticStoreFactory</sv:value>
      </sv:property>
      <sv:property sv:name="indexName" sv:type="String">
        <sv:value>requests</sv:value>
      </sv:property>
      <sv:property sv:name="locations" sv:type="String" sv:multiple="true">
        <sv:value>http://localhost:9200</sv:value>
      </sv:property>
    </sv:node>
    <sv:node sv:name="targeting:targetingdata" esv:merge="combine">
