    <sv:property sv:name="jcr:primaryType" sv:type="Name">
      <sv:value>targeting:trends</sv:value>
    </sv:property>
    <sv:node sv:name="targeting:facets">
      <sv:property sv:name="jcr:primaryType" sv:type="Name">
