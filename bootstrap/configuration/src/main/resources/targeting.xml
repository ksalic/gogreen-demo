      </sv:property>
      <sv:node sv:name="category">
        <sv:property sv:name="jcr:primaryType" sv:type="Name">
          <sv:value>targeting:facet</sv:value>
        </sv:property>
        <sv:property sv:name="targeting:esField" sv:type="String">
          <sv:value>requests.collectorData.categories.terms</sv:value>
        </sv:property>
        <sv:property sv:name="targeting:facetName" sv:type="String">
          <sv:value>category</sv:value>
        </sv:property>
        <sv:property sv:name="targeting:filterName" sv:type="String">
